{
  "name": "backend",
  "version": "0.0.0",
  "private": true,
  "type": "module",
  "scripts": {
    "start": "node ./index.js",
    "dev": "nodemon ./index.js",
    "lint": "eslint  *.js src/*.js src/**/*.js --quiet --fix"
  },
  "dependencies": {
    "cookie-parser": "~1.4.4",
    "cors": "^2.8.5",
    "debug": "~2.6.9",
    "dotenv": "^16.0.1",
    "express": "~4.16.1",
    "http-errors": "~1.6.3",
    "jade": "~1.11.0",
    "mongoose": "^6.3.6",
    "morgan": "~1.9.1",
    "nodemon": "^2.0.16"
  },
  "devDependencies": {
    "eslint": "^8.17.0",
    "eslint-config-airbnb-base": "^15.0.0"
  },
  "eslintConfig": {
    "extends": "airbnb-base",
    "env": {
      "es6": true,
      "node": true
    },
    "rules": {
      "brace-style": [
        "error",
        "stroustrup"
      ],
      "indent": [
        "error",
        4
      ],
      "no-unused-vars": [
        "warn"
      ],
      "no-param-reassign": [
        "off"
      ],
      "no-underscore-dangle": [
        "off"
      ],
      "no-trailing-spaces": [
        "warn"
      ],
      "import/extensions": [
        "off"
      ]
    }
  }
}
